         <type>INTEGER</type>
         <type>INTEGER</type>
         <type>INTEGER</type>
         <type>INTEGER</type>
         <type>TIMESTAMP</type>
         <type>FLOAT</type>
         <type>VARCHAR</type>
      </types>
      <values>
         <value>1</value>
