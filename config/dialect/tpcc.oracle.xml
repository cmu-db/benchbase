<?xml version="1.0" encoding="utf-8"?>
<dialect xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" type="ORACLE" xsi:noNamespaceSchemaLocation="dialect.xsd">
    <procedures>
        <procedure procedureClass="com.oltpbenchmark.benchmarks.tpcc.procedures.Delivery">
            <statements>
                <statement name="delivGetOrderIdSQL">
                    SELECT NO_O_ID
                    FROM NEW_ORDER
                    WHERE NO_D_ID = ? AND NO_W_ID = ? AND ROWNUM = 1 ORDER BY NO_O_ID ASC
                </statement>
            </statements>
        </procedure>
        <procedure procedureClass="com.oltpbenchmark.benchmarks.tpcc.procedures.OrderStatus">
            <statements>
                <statement name="ordStatGetNewestOrdSQL">
                    SELECT O_ID, O_CARRIER_ID, O_ENTRY_D
                    FROM OORDER
                    WHERE O_W_ID = ? AND O_D_ID = ? AND O_C_ID = ? AND ROWNUM =1 ORDER BY O_ID DESC
                </statement>
            </statements>
        </procedure>
    </procedures>
</dialect>
