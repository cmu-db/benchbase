<?xml version="1.0" encoding="utf-8"?>
<dialect xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" type="CASSANDRA" xsi:noNamespaceSchemaLocation="dialect.xsd">
    <procedures>
        <procedure procedureClass="com.oltpbenchmark.benchmarks.ycsb.procedures.ScanRecord">
            <statements>
                <statement name="scanStmt">
                    SELECT * FROM USERTABLE WHERE YCSB_KEY &gt; ? AND YCSB_KEY &lt; ? ALLOW FILTERING
                </statement>
            </statements>
        </procedure>
        <procedure procedureClass="com.oltpbenchmark.benchmarks.ycsb.procedures.InsertRecord">
            <statements>
                <statement name="insertStmt">
                    INSERT INTO USERTABLE (YCSB_KEY, FIELD1, FIELD2, FIELD3, FIELD4, FIELD5, FIELD6, FIELD7, FIELD8, FIELD9, FIELD10) VALUES (?,?,?,?,?,?,?,?,?,?,?)
                </statement>
            </statements>
        </procedure>
    </procedures>
</dialect>
