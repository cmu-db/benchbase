<?xml version="1.0"?>
<dialect xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" type="ORACLE" xsi:noNamespaceSchemaLocation="dialect.xsd">
    <procedures>
        <procedure name="LoadConfig">
            <statements>
                <statement name="getFlights">
                    SELECT f_id FROM FLIGHT WHERE ROWNUM &lt;= 10000 ORDER BY F_DEPART_TIME DESC
                </statement>
            </statements>
        </procedure>
    </procedures>
</dialect>
