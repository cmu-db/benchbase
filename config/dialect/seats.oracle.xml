<?xml version="1.0"?>
<dialect type="ORACLE" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="dialect.xsd">
    <procedures>
        <procedure name="LoadConfig">
            <statement name="getFlights">
                SELECT f_id FROM FLIGHT WHERE ROWNUM &lt;= 10000 ORDER BY F_DEPART_TIME DESC
            </statement>
        </procedure>
    </procedures>
</dialect>
