<?xml version="1.0" encoding="utf-8"?>
<configuration xsi:noNamespaceSchemaLocation="workload.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <workloads>
        <workload benchmarkClass="com.oltpbenchmark.benchmarks.voter.VoterBenchmark">

            <scaleFactor>1</scaleFactor>
            <terminals>1</terminals>

            <phases>
                <phase>
                    <time>60</time>
                    <rateType>LIMITED</rateType>
                    <rate>10000</rate>
                    <weight>100</weight>
                </phase>
            </phases>

            <transactions>
                <transaction procedureClass="com.oltpbenchmark.benchmarks.voter.procedures.Vote"/>
            </transactions>
        </workload>
    </workloads>

</configuration>
