<?xml version="1.0" encoding="utf-8"?>
<configuration xsi:noNamespaceSchemaLocation="workload.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <workloads>
        <workload benchmarkClass="com.oltpbenchmark.benchmarks.resourcestresser.ResourceStresserBenchmark">
            <scaleFactor>1</scaleFactor>
            <terminals>1</terminals>

            <phases>
                <phase>
                    <time>60</time>
                    <rateType>LIMITED</rateType>
                    <rate>10000</rate>
                    <weight>16.66,16.66,16.66,16.66,16.66,16.7</weight>
                </phase>
            </phases>

            <transactions>
                <transaction procedureClass="com.oltpbenchmark.benchmarks.resourcestresser.procedures.CPU1">
                    <name>CPU1</name>
                </transaction>
                <transaction procedureClass="com.oltpbenchmark.benchmarks.resourcestresser.procedures.CPU2">
                    <name>CPU2</name>
                </transaction>
                <transaction procedureClass="com.oltpbenchmark.benchmarks.resourcestresser.procedures.IO1">
                    <name>IO1</name>
                </transaction>
                <transaction procedureClass="com.oltpbenchmark.benchmarks.resourcestresser.procedures.IO2">
                    <name>IO2</name>
                </transaction>
                <transaction procedureClass="com.oltpbenchmark.benchmarks.resourcestresser.procedures.Contention1">
                    <name>Contention1</name>
                </transaction>
                <transaction procedureClass="com.oltpbenchmark.benchmarks.resourcestresser.procedures.Contention2">
                    <name>Contention2</name>
                </transaction>
            </transactions>
        </workload>
    </workloads>


</configuration>
